{
  "name": "anilla-cigar-explorer",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ios:clean": "rm -rf ios && rm -rf node_modules && npm install && npx expo prebuild --clean",
    "ios:pods": "cd ios && pod install --repo-update && cd ..",
    "ios:build": "npx expo prebuild -p ios && cd ios && pod install && open anillacigarexplorer.xcworkspace"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.39.3",
    "chroma-js": "^3.1.2",
    "d3-shape": "^3.2.0",
    "expo": "52.0.33",
    "expo-blur": "^14.0.3",
    "expo-constants": "^17.0.5",
    "expo-font": "~13.0.3",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "^16.0.6",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-router": "^4.0.17",
    "expo-splash-screen": "^0.29.21",
    "expo-status-bar": "^2.0.1",
    "expo-symbols": "^0.2.2",
    "expo-system-ui": "^4.0.7",
    "expo-web-browser": "^14.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "^2.23.1",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "^4.4.0",
    "react-native-svg": "^15.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.19.13",
    "react-native-webview": "13.12.5",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/chroma-js": "^3.1.1",
    "@types/d3-shape": "^3.1.7",
    "@types/dom-speech-recognition": "^0.0.4",
    "@types/node": "^22.13.4",
    "@types/react": "~18.3.12",
    "@types/react-native": "^0.72.8",
    "typescript": "^5.7.3"
  }
